<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/23.0.1" fx:controller="md.ceiti.md.cazarefx_hibernate.view.Clienti.DataClientController">
    <Region prefHeight="10" />
    <HBox spacing="10">
        <TextField fx:id="searchField" prefHeight="26.0" prefWidth="214.0" promptText="Search..." />
    </HBox>
    <Region prefHeight="10" />
    <TableView fx:id="clientTable" VBox.vgrow="ALWAYS">
        <columns>
            <TableColumn fx:id="nameColumn" text="Name" />
            <TableColumn fx:id="emailColumn" text="Email" />
            <TableColumn fx:id="telefonColumn" text="Phone" />
            <TableColumn fx:id="genColumn" text="Gender" />
            <TableColumn fx:id="categorieColumn" text="Category" />
        </columns>
    </TableView>
</VBox>
